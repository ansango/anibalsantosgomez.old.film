    "nextjs-google-analytics": "^2.3.3",
    "nodemailer": "^6.9.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-wrap-balancer": "^0.4.0",
    "sonner": "^0.3.0",
    "tinacms": "^1.4.6",
